#
# LinkedIDS Schema / Mappings
#
# V. de Boer and C. Gueret March 2012
#
@prefix rdfs:     <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos:     <http://www.w3.org/2004/02/skos/core#> .
@prefix ens:      <http://www.europeana.eu/schemas/edm/> .
@prefix ontology: <http://www.geonames.org/ontology#> .
@prefix dcterms:  <http://purl.org/dc/terms/> .
@prefix rdf:      <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

# --------------------------- IDSResource -----------------------
:IDSResource
	a rdfs:Class ;
	rdfs:label "Generic IDS Resource" ;
	rdfs:subClassOf rdfs:Resource.

:title
	a rdf:Property ;
	rdfs:domain :IDSResource ; 
	rdfs:label "title" ; 
	rdfs:subPropertyOf rdfs:label ; 
	rdfs:comment "title of the Resource (IDS-generic)".
	
:object_id	
	a rdf:Property ;
	rdfs:domain :IDSResource ; 
	rdfs:label "object id" ; 
	rdfs:comment "object id of the Resource (IDS-generic)".
	
:object_type
	a rdf:Property ;
	rdfs:domain :IDSResource ; 
	rdfs:label "title" ; 
	rdfs:subPropertyOf rdf:type;
	rdfs:comment "type of the Resource (IDS-generic)".
	
:metadata_url
	a rdf:Property ;
	rdfs:domain :IDSResource ; 
	rdfs:label "metadata url" ; 
	rdfs:subPropertyOf rdfs:seeAlso ;
	rdfs:comment "url of the metadata for the Resource (IDS-generic)".

:timestamp
	a rdf:Property ;
	rdfs:domain :IDSResource ;
	rdfs:label "timestamp" ;
	rdfs:range rdfs:Literal ;
	rdfs:comment "Indicates when record was indexed in the API. Used to determine updates to the index for synchronisation. Timestamp is in ISO 8601 format: YYYY-MM-DD hh:mm:ss." .
	
:website_url
	a rdf:Property ;
	rdfs:domain :IDSResource ;
	rdfs:label "website url" ;
	rdfs:range rdfs:Resource ;
	rdfs:comment "URL for the asset on the collection website";
	rdfs:subPropertyOf rdfs:seeAlso.	
	
# Next two are for documents and organisations	
:date_created
	a rdf:Property ;
	rdfs:label "Date created" ;
	rdfs:subPropertyOf dcterms:created.
	
# VIC: subprop created? or something else
:date_updated
	a rdf:Property ;
	rdfs:label "Date updated" ;
	rdfs:subPropertyOf dcterms:created.

	
# ---------------------------  Country --------------------------------

:Country
	a rdfs:Class ;
	rdfs:label "Country" ;
	rdfs:subClassOf ontology:Feature.

:alternative_name
	a rdf:Property ;
	rdfs:domain :Country ;
	rdfs:label "alternative_name" ;
	rdfs:range rdfs:Literal ;
	rdfs:subPropertyOf ontology:alternateName.

:asset_id
	a rdf:Property ;
	rdfs:domain :Country ;
	rdfs:label "asset_id" ;
	rdfs:range rdfs:Literal ;
	rdfs:subPropertyOf dcterms:identifier.

:country_name
	a rdf:Property ;
	rdfs:domain :Country ;
	rdfs:label "country name" ;
	rdfs:range rdfs:Literal ;
	rdfs:subPropertyOf ontology:name.

:iso_number
	a rdf:Property ;
	rdfs:domain :Country ;
	rdfs:label "iso number" ;
	rdfs:range rdfs:Literal ;
	rdfs:subPropertyOf dcterms:identifier.
	
:iso_three_letter_code
	a rdf:Property ;
	rdfs:domain :Country ;
	rdfs:label "iso three letter code" ;
	rdfs:range rdfs:Literal ;
	rdfs:subPropertyOf dcterms:identifier.

:iso_two_letter_code
	a rdf:Property ;
	rdfs:domain :Country ;
	rdfs:label "ISO two letter code" ;
	rdfs:range rdfs:Literal ;
	rdfs:subPropertyOf ontology:countryCode.

:region
	a rdf:Property ;
	rdfs:domain :Country ;
	rdfs:range :Region ;
	rdfs:label "region of the country" ;
	rdfs:subPropertyOf ontology:parentFeature.

	
# ---------------------------  Regions --------------------------------

:Region
	a rdfs:Class ;
	rdfs:label "Region" ;
	rdfs:subClassOf ontology:Feature.

:cat_first_parent
	a rdf:Property .
:cat_level
	a rdf:Property .
:cat_superparent
	a rdf:Property .
:category_id
	a rdf:Property .
:category_path	
	a rdf:Property .


 

# ------------------------- Documents ----------------------------------

:Document
	a rdfs:Class;
	rdfs:label "Document" ;
	rdfs:subClassOf :IDSResource;
	rdfs:subClassOf foaf:Document .
 
:category_region
	a rdf:Property ;
	rdfs:label "Region of the document" ;
	rdfs:subPropertyOf dcterms:coverage.
	
:category_theme
	a rdf:Property ;
	rdfs:label "Theme of the document" ;
	rdfs:subPropertyOf dcterms:subject.
	
:country_focus
	a rdf:Property ;
	rdfs:label "Country of the document" ;
	rdfs:subPropertyOf dcterms:coverage.
	
:description
	a rdf:Property ;
	rdfs:subPropertyOf dcterms:description;
	rdfs:label "Description" .
	
:keyword
	a rdf:Property ;
	rdfs:label "Subject keywords that relate to the document" ;
	rdfs:subPropertyOf dcterms:subject.
	
# VIC make this more semantic-> enrich to language resources
:language_name 
	a rdf:Property ;
	rdfs:label "The language in which the title, headline and description are written" ;
	rdfs:subPropertyOf dcterms:language.
	
:licence_type
	a rdf:Property ;
	rdfs:label "Type of licence that document is released under " ;
	rdfs:subPropertyOf dcterms:license.

# VIC: maybe contextualize this? 
:name
	a rdf:Property ;
	rdfs:label "Name of the document" ;
	rdfs:subPropertyOf dcterms:title.
	
:publication_date
	a rdf:Property ;
	rdfs:label "Date the research document was first published" ;
	rdfs:subPropertyOf dcterms:created.
	
:publication_year
	a rdf:Property ;
	rdfs:label "Year the document was first published" ;
	rdfs:subPropertyOf dcterms:created.
	
# VIC: publishers are classes!	
:publisher
	a rdf:Property ;
	rdfs:label "Organisation that published the research. Each publisher has a linked organisation record, containing address, website and additional information" ;
	rdf:range :Organisation;
	rdfs:subPropertyOf dcterms:publisher.
	 
:title_sort
	a rdf:Property ;
	rdfs:label "Title on which to sort" ;
	rdfs:subPropertyOf dcterms:title.
	
:urls
	a rdf:Property ;
	rdfs:label "URLs of the full text document" ;
	rdfs:subPropertyOf dcterms:source ; 
	rdf:range rdf:Resource.
	
	
#-------------------------- Organisations ---------------------------------	

:Organisation 
	a rdfs:Class ;
	rdfs:subClassOf foaf:Organisation ;
	rdfs:label "Organisation".

:acronym
	a rdf:Property ;
	rdfs:label "Acronym of organisation" ;
	rdfs:subPropertyOf foaf:name.
	
:alternative_acronym
	a rdf:Property ;
	rdfs:label "Alternative acronym of organisation" ;
	rdfs:subPropertyOf foaf:name.
	
:alternative_name
	a rdf:Property ;
	rdfs:label "Alternative name of organisation" ;
	rdfs:subPropertyOf foaf:name.
	
# VIC:category_region, category_theme, country_focus, description, keywords are retained from documents?

# VIC: dcterms type? -> There seems to be such a thing as an organisation type, although the link on the site that should display the list of organisation types doesnt work
:organisation_type
	a rdf:Property ;
	rdfs:label "Type of organisation" ;
	#rdf:range :OrganisationType ;
	rdf:range rdfs:Literal ;
	rdfs:subPropertyOf dcterms:type.

:organisation_url
	a rdf:Property ;
	rdfs:label "Website of the organisation" ;
	rdfs:subPropertyOf foaf:homepage; 
	rdf:range rdf:Resource.

